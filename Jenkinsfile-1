pipeline {
    agent any
    stages {
        stage ('Initialize') {
            steps {
                echo "Initializing the Code file.."
            }
        }

        stage ('Build') {
            steps {
                echo 'Hello World..!!'
            }
        }

        stage ('Deploy') {
            steps {
                echo 'Deployed an Artifact..'
            }
        }
    }
}
